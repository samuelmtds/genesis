<project>
    <extend>../../project.xml</extend>
    <artifactId>genesis-container</artifactId>
    <name>Genesis Container</name>
    <repository>
      <connection>
         scm:cvs:pserver:${maven.guest.username}@cvs.dev.java.net:/cvs:genesis</connection>
      <developerConnection>
         scm:cvs:pserver:${maven.username}@cvs.dev.java.net:/cvs:genesis</developerConnection>
      <url>https://genesis.dev.java.net/source/browse/genesis/genesis/modules/container/</url>
    </repository>
    <dependencies>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.0.3</version>
            <type>jar</type>
            <url>http://jakarta.apache.org/commons/</url>
            <properties/>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src</sourceDirectory>
        <unitTest />
    </build>
    <reports>
        <report>maven-simian-plugin</report>
        <report>maven-junit-report-plugin</report>
        <report>maven-statcvs-plugin</report>
        <report>maven-file-activity-plugin</report>
        <report>maven-developer-activity-plugin</report>
        <report>maven-jxr-plugin</report>
        <report>maven-javadoc-plugin</report>
    </reports>
    <properties/>
</project>