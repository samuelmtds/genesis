<?xml version="1.0" encoding="UTF-8"?>

<!--
<!DOCTYPE xdoclet-module PUBLIC "-//XDoclet Team//DTD XDoclet Module 1.0//EN" "http://xdoclet.sourceforge.net/dtd/xdoclet-module_1_0.dtd">
-->

<xdoclet-module>
    <!--
    xdoclet.modules.hibernate.JBossServiceSubTask
    xdoclet.modules.hibernate.HibernateSubTask
    xdoclet.modules.hibernate.HibernateTagsHandler
    xdoclet.modules.hibernate.FactoryClassSubTask
    xdoclet.modules.hibernate.HibernateSubTask.HibernateVersion
    -->

    <taghandler
        namespace="Hibernate"
        class="xdoclet.modules.hibernate.HibernateTagsHandler"
    />

    <subtask
        name="jbossservice"
        implementation-class="xdoclet.modules.hibernate.JBossServiceSubTask"
        parent-task-class="xdoclet.modules.hibernate.HibernateDocletTask"
    />
    <subtask
        name="hibernate"
        implementation-class="xdoclet.modules.hibernate.HibernateSubTask"
        parent-task-class="xdoclet.modules.hibernate.HibernateDocletTask"
    />
    <subtask
        name="factoryclass"
        implementation-class="xdoclet.modules.hibernate.FactoryClassSubTask"
        parent-task-class="xdoclet.modules.hibernate.HibernateDocletTask"
    />

    <subtask
        name="hibernatecfg"
        implementation-class="xdoclet.modules.hibernate.HibernateCfgSubTask"
        parent-task-class="xdoclet.modules.hibernate.HibernateDocletTask"
    />

</xdoclet-module>
